{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "css.lint.unknownAtRules": "ignore",
  "eslint.packageManager": "yarn",
  "eslint.alwaysShowStatus": true,
  "eslint.validate": [
    "vue",
    "jsx",
    "tsx",
    "ts",
    "typescript",
    "html",
    "javascript",
    "javascriptreact",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#9f0fff",
    "activityBar.activeBorder": "#945bc4",
    "activityBar.background": "#9f0fff",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#945bc4",
    "activityBarBadge.foreground": "#e7e7e7",
    "statusBar.background": "#9f0fff",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#ffffff",
    "titleBar.activeBackground": "#9f0fff",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#9f0fff99",
    "titleBar.inactiveForeground": "#15202b99",
    "sash.hoverBorder": "#9f0fff",
    "statusBarItem.remoteBackground": "#9f0fff",
    "statusBarItem.remoteForeground": "#15202b",
    "commandCenter.border": "#15202b99"
  },
  "peacock.color": "#9f0fff"
}
