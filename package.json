    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.12",
    "ts-node": "^10.9.2",
    "tsx": "^4.11.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.18.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.23.0",
    "graphql-yoga": "^5.3.1",
    "husky": "^9.0.11",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "pg": "^8.11.5",
    "prettier": "^3.2.5",
    "zod": "^3.23.8"
  },
