{
  "name": "back-lizardium",
  "version": "1.0.0",
  "author": "IgorGabrielNunes <profissional.igornunes@gmail.com>",
  "license": "PROPRIETARY",
  "main": "index.js",
  "type": "commonjs",
  "private": true,
  "packageManager": "yarn@3.5.0",
  "engines": {
    "node": "^18"
  },
  "description": "",
  "scripts": {
    "dev": "yarn generate && nodemon -r tsconfig-paths/register src/index.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "build": "yarn run build:clean && yarn run generate && yarn tsc && yarn tsc-alias",
    "build:watch": "yarn run build:clean && yarn tsc && (npx concurrently \"yarn tsc -w\" \"yarn tsc-alias -w\")",
    "build:clean": "rm -rf dist/ && rm -rf tsconfig.tsbuildinfo && yarn run generate",
    "generate": "graphql-codegen --config codegen.ts",
    "knex": "ts-node -r dotenv/config ./node_modules/.bin/knex --knexfile ./src/infra/db/knexfile.ts",
    "db:prepare": "yarn knex migrate:latest",
    "test": "vitest -c vitest.unit.config.ts",
    "coverage": "vitest run --coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/server": "^4.8.0",
    "@as-integrations/fastify": "^1.3.2",
    "@fastify/cors": "^8.3.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/luxon": "^3.3.1",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.1",
    "dataloader": "^2.2.2",
    "dotenv": "^16.3.1",
    "fastify": "^4.20.0",
    "graphql": "^16.9.0",
    "graphql-scalars": "^1.22.2",
    "http-terminator": "^3.2.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "luxon": "^3.3.0",
    "pg": "^8.11.1",
    "pino": "^8.14.1",
    "pm2": "^5.3.0",
    "tslib": "^2.6.0"
  },
  "devDependencies": {
    "@eddeee888/gcg-typescript-resolver-files": "^0.3.0",
    "@faker-js/faker": "^7.6.0",
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/typescript": "^3.0.4",
    "@graphql-codegen/typescript-resolvers": "^3.2.1",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@vitest/coverage-v8": "^0.32.4",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "git-format-staged": "^3.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "nodemon": "^2.0.22",
    "pino-pretty": "^10.1.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "latest",
    "vitest": "^0.33.0"
  }
}
